<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
    	<theme>dark</theme>
		<encoding>UTF-8</encoding>
		<protocol>https</protocol>
        <documentTitle>RT2 Automation</documentTitle>
        <reportName>
        <!-- ADROSONIC Automation Report This will be your report heading -->
        <![CDATA[
         
        ]]>
         <!-- <img src='https://test-evantage.risktransfergroup.com/RiskTracker/img/RiskTracker.LogoBlue.png'/>   -->    
        <![CDATA[ <h7> RT2 Automation Report </h7> ]]>
        </reportName>
        
        <!-- report headline - displayed at top-nav, after reportHeadline -->
        <reportHeadline></reportHeadline>
        
        <!-- global date format override -->
        <!-- defaults to yyyy-MM-dd -->
        <dateFormat>dd-MMM-yyyy</dateFormat>
        
        <!-- global time format override -->
        <!-- defaults to HH:mm:ss -->
        <timeFormat>HH:mm:ss</timeFormat>
        
        <!-- custom javascript -->
        <scripts>
            <![CDATA[
                $(document).ready(function() {
                   $('.brand-logo black').css('display','none'); 
                });
            ]]>
            
        </scripts>
        
        <!-- custom styles -->
        <styles>
            <![CDATA[
                .report-name { padding-left: 40px; } .report-name > img { float: left;height: auto;margin-left: 15px;margin-top: 6px;width: 300px; }
                .report-name > h7 {margin-left: 10px; margin-bottom: 10px; }
            ]]>
        </styles>
    </configuration>
</extentreports>